{
    "compilerOptions": {
        "target": "ES2015",
        "allowJs": false,
        "strict": true,
        "skipLibCheck": true,
        "module": "commonjs",
        "importHelpers": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}
